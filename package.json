{
  "name": "pockets",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "type":"module",
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "ohm": "ohm generateBundles --withTypes src/ohm/diceroll.ohm",
    "test": "jest"
  },
  "devDependencies": {
    "@ohm-js/cli": "^1.1.0",
    "@parcel/transformer-typescript-tsc": "^2.8.3",
    "@types/jest": "^29.5.1",
    "jest": "^29.5.0",
    "parcel": "^2.8.3",
    "parcel-transformer-ts-css-modules": "^1.0.14",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "comlink": "^4.4.1",
    "ohm-js": "^17.0.4",
    "preact": "^10.14.1",
    "true-myth": "^6.2.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleNameMapper": {
      ".+\\.css$": "<rootDir>/src/styleMock.ts"
    },
    "testMatch": [
      "**/__tests__/*.(test|spec).(ts|tsx|js)"
    ]
  },
  "alias": {
    "$": "./src",
    "lib": "./src/lib",
    "ohm": "./src/ohm",
    "react": "preact/compat",
    "react-dom": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react/jsx-runtime": "preact/jsx-runtime"
  }
}
